{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "qm-base",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "qm-app2": {
      "tags": ["scope:store", "type:app"]
    },
    "qm-app2-e2e": {
      "tags": [],
      "implicitDependencies": ["qm-app2"]
    },
    "ui": {
      "tags": ["scope:store", "type:ui"]
    },
    "ui-e2e": {
      "tags": [],
      "implicitDependencies": ["ui"]
    },
    "util-formatters": {
      "tags": ["scope:shared", "type:util"]
    },
    "qm-app3": {
      "tags": []
    },
    "qm-app3-e2e": {
      "tags": [],
      "implicitDependencies": ["qm-app3"]
    }
  }
}
